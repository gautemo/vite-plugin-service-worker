  "repository": {
    "type": "git",
    "url": "git+https://github.com/gautemo/vite-plugin-service-worker.git"
  },
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "author": "Gaute Meek Olsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gautemo/vite-plugin-service-worker/issues"
  },
  "homepage": "https://github.com/gautemo/vite-plugin-service-worker#readme",
  "devDependencies": {
    "@types/node": "^22.0.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  }
